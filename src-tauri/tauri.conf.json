{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "SquirrelDisk",
    "version": "1.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": true,
      "fs": {
        "all": false,
        "copyFile": false,
        "createDir": false,
        "exists": false,
        "readDir": false,
        "readFile": false,
        "removeDir": true,
        "removeFile": true,
        "renameFile": false,
        "scope": [
          "**"
        ],
        "writeFile": false
      }
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [
        "binaries/pdu"
      ],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/256x256.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.squirreldisk.dev",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "resizable": true,
        "title": "SquirrelDisk",
        "width": 1100,
        "decorations": false,
        "transparent": true,
        "center": true
      }
    ]
  }
}